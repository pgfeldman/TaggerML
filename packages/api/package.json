  "keywords": [
    "machine learning",
    "textual tagging"
  ],
  "repository": {
    "type": "git",
    "url": "git@github.com:pgfeldman/TaggerML.git"
  },
  "scripts": {
    "build": "rm -rf dist && tsc",
    "clean": "rm -rf node_modules",
    "dev": "concurrently 'yarn local-start' 'yarn playground'",
    "debug": "dotenv -- nodemon -e ts,graphql -x ts-node --project tsconfig.json --inspect src/index.ts",
    "docker:up:dev": "docker-compose up -d; sleep 5s",
    "docker:up:test": "docker-compose -f docker-compose.e2e.yml up -d",
    "docker:down": "docker-compose down",
    "local-start": "dotenv -e .env.local -- nodemon -e ts,graphql -x ts-node --project tsconfig.json src/index.ts",
    "now-start": "dotenv -- nodemon -e ts,graphql -x NODE_ENV=production node dist/index.js",
    "playground": "graphql playground --dotenv .env.local --port 8000",
    "schema:sync": "dotenv -e .env.local -- ts-node ./node_modules/.bin/typeorm schema:sync"
  },
  "dependencies": {
    "apollo-datasource": "^0.2.1",
    "apollo-server": "^2.3.0",
    "glob": "^7.1.3",
    "graphql": "^14.0.2",
    "graphql-tools": "^4.0.3",
    "merge-graphql-schemas": "^1.5.8",
    "mysql": "^2.16.0",
    "reflect-metadata": "^0.1.12",
    "typeorm": "^0.2.9"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.2",
    "@types/dotenv": "^6.1.0",
    "@types/glob": "^7.1.1",
    "@types/graphql": "^14.0.3",
    "@types/jsonwebtoken": "^8.3.0",
    "@types/mysql": "^2.15.5",
    "concurrently": "^4.1.0",
    "dotenv": "^6.2.0",
    "dotenv-cli": "^1.4.0",
    "graphql-cli": "^2.17.0",
    "nodemon": "^1.18.8",
    "ts-node": "^7.0.1"
  }
}
